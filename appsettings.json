{
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=PepegaFoodDB;Trusted_Connection=True;MultipleActiveResultSets=true"
  },

  "PasswordRequireOptions": {
    "RequireDigit": "true",
    "RequireNonAlphanumeric": "false",
    "RequiredLength": "6",
    "RequireUppercase": "false",
    "RequiredUniqueChars": "1",
    "RequireLowercase": "false"
  },

  "SwaggerOptions": {
    "JsonRoute": "swagger/{documentName}/swagger.json",
    "Description": "API for PepegaFood",
    "UIEndpoint": "v0.1/swagger.json"
  },

  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
